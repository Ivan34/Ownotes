{
  "manifest_version": 2,

  "name": "Ownotes",
  "description": "__MSG_description__",
  "version": "1.0",
  "default_locale": "en",
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["jquery.js", "actions.js", "options.js"],
      "matches": ["http://*/*", "https://*/*"]
    } ]
}
